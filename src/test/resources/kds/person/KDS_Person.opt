        </term_definitions>
        <term_definitions code="at0009">
            <items id="text">Verwandte</items>
            <items id="description">Zeigt an, dass der Tod von einem Verwandten gemeldet wurde.</items>
        </term_definitions>
        <term_definitions code="at0010">
            <items id="text">Eine andere Quelle</items>
            <items id="description">Zeigt an, dass der Tod von einer anderen Quelle gemeldet wurde.</items>
        </term_definitions>
