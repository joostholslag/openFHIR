        </term_definitions>
        <term_definitions code="at0001">
            <items id="text">Sterbedatum</items>
            <items id="description">Der Zeitpunkt, an dem die Person verstorben ist. </items>
        </term_definitions>
        <term_definitions code="at0002">
            <items id="text">Tod aufgrund der Primärdiagnose?</items>
            <items id="description">Ist der Tod auf die Primärdiagnose zurückzuführen?</items>
        </term_definitions>
        <term_definitions code="at0003">
            <items id="text">Todesdiagnose</items>
            <items id="description">Die Ursache, welche zum Tode geführt hat.</items>
            <items id="comment">Die Todesursache sollte, wenn möglich, mit einer Terminologie kodiert werden.</items>
        </term_definitions>
        <term_definitions code="at0004">
            <items id="text">Kommentar</items>
            <items id="description">Ergänzende Angaben über den Tod, die nicht in anderen Bereichen erfasst wurden.</items>
        </term_definitions>
        <term_definitions code="at0005">
            <items id="text">Spezifische Angaben</items>
            <items id="description">Spezifische strukturierte Angaben über den Tod.</items>
        </term_definitions>
        <term_definitions code="at0006">
            <items id="text">Informationsquelle</items>
            <items id="description">Gibt an, woher die Informationen über den Tod stammen und wie sicher diese Informationen sind.</items>
        </term_definitions>
        <term_definitions code="at0007">
            <items id="text">Sterberegister</items>
            <items id="description">Der Tod wurde von einem Sterberegister/notariell gemeldet.</items>
        </term_definitions>
        <term_definitions code="at0008">
            <items id="text">Gesundheitsdienstleister</items>
